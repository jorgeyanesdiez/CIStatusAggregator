{
  "Endpoints": [{
    "Meta": {
      "Description": "Build jobs"
    },
    "Remote": {
      "BaseUrl": "https://jenkins.mycompany.com",
      "JobNameFilterRegex": "build",
      "JobNameFilterMode": "Whitelist"
    },
    "Local": {
      "StatusFilePath": "status/build.json"
    }
  },
  {
    "Meta": {
      "Description": "Deployment jobs"
    },
    "Remote": {
      "BaseUrl": "https://jenkins.mycompany.com",
      "JobNameFilterRegex": "build",
      "JobNameFilterMode": "Blacklist"
    },
    "Local": {
      "StatusFilePath": "status/deploy.json"
    }
  },
  {
    "Meta": {
      "Description": "Project2 jobs"
    },
    "Remote": {
      "BaseUrl": "https://jenkins.mycompany.com/view/project2"
    },
    "Local": {
      "StatusFilePath": "status/project2.json"
    }
  }],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  }
}
